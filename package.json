{
  "name": "lil-match",
  "version": "1.2.1",
  "description": "Super small pattern matching library for TS projects",
  "type": "module",
  "main": "./mod.js",
  "types": "./mod.d.ts",
  "scripts": {
    "test": "tsc --noEmit && vitest"
  },
  "repository": "exah/lil-match",
  "author": "John Grishin <r@exah.me>",
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "pattern",
    "matching",
    "typescript",
    "tiny",
    "small",
    "nano",
    "little",
    "lil",
    "match"
  ],
  "files": [
    "mod.js",
    "mod.d.ts"
  ],
  "size-limit": [
    {
      "name": "exports",
      "path": "./mod.js",
      "import": "{ match, when, list }",
      "limit": "280B"
    },
    {
      "name": "match",
      "path": "./mod.js",
      "import": "{ match }",
      "limit": "240B"
    },
    {
      "name": "when",
      "path": "./mod.js",
      "import": "{ when }",
      "limit": "25B"
    },
    {
      "name": "list",
      "path": "./mod.js",
      "import": "{ list }",
      "limit": "180B"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "np": "^7.6.0",
    "size-limit": "^7.0.5",
    "tsd": "^0.19.0",
    "type-fest": "^2.8.0",
    "typescript": "^4.5.4",
    "vitest": "^0.4.1"
  }
}
